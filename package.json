{
  "name": "d3-labels",
  "version": "0.0.1",
  "description": "Place labels with d3js on maps and other vizualisations",
  "main": "build/d3-labels.js",
  "jsnext:main": "index",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "rollup --banner \"$(preamble)\" -f umd -n d3 -o build/d3-labels.js -- index.js",
    "test": "tape 'test/*-test.js' && mkdir -p test/output && eslint index.js src"
  },
  "keywords": [
    "d3",
    "label",
    "labels"
  ],
  "author": {
    "name": "Roger Veciana i Rovira",
    "url": "https://geoexamples.com"
  },
  "license": "ISC",
  "devDependencies": {
    "canvas": "^1.6.2",
    "package-preamble": "0.0.2",
    "rollup": "^0.41.1",
    "tape": "^4.6.3"
  }
}
